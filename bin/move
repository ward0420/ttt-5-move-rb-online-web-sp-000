#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts "Welcome to Tic Tac Toe!"
puts "Where would you like to go?"
board = [" ", " ", " ", " ", " ", " ", " ", " ", " ",]
input_to_index(user_input = gets.strip)
move(board, position = 0, character = "X")
display_board(board)